#!/bin/bash
########################################################################3
# SUGOS REMASTER SCRIPT FOR MAKE DISTRO FROM ISO FILE
# AUTHOR    : SUGOS LINUX
# LICENSE   : GNU GPL 3.0
# WEB       : https://github.com/SUGOS-GNU-Linux
########################################################################

# Use the following command if the exit from the chroot
apt-get clean && apt-get autoremove && rm -rf /tmp/* ~/.bash_history && umount /proc && umount /sys && umount /dev/pts
exit
sudo umount edit/dev && sudo umount mnt
